{
  "name": "travoofy-api-v2",
  "version": "1.0.0",
  "description": "travoofy-api-v2",
  "main": "app.js",
  "scripts": {
    "test": "jest",
    "start": "node ./build/app.js",
    "dev": "nodemon --files ./src/app.ts",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run lint && npm run build:types && npm run build:js && node copyMockData.js",
    "lint:fix": "npm run lint:js -- --fix",
    "deploy": "npm install && npm run lint && npm run build:types && npm run build:js",
    "fix": "eslint --fix \"src/**/*.{ts,tsx}\"",
    "audit": "npm audit fix",
    "build:types": "tsc --emitDeclarationOnly --declarationDir build",
    "build:js": "babel src --out-dir build --extensions \".ts,.tsx\" --source-maps inline",
    "lint": "eslint eslint.config.js src/**/*.{jsx,ts,tsx} --fix",
    "update-deps": "npx npm-check-updates -u && npm install",
    "check-deps": "depcheck",
    "mvc": "tsc ./generate-service.ts && node generate-service.js",
    "mv": "tsc ./generateService.ts && node generateService.js",
    "start:dev": "cross-env NODE_ENV=staging nodemon --files ./src/app.ts",
    "start:prod": "cross-env NODE_ENV=production node -r dotenv/config ./build/app.js",
    "seed": "nodemon ./src/services/crons/seeder/mainSeeder.ts",
    "seed:users": "cross-env NODE_ENV=staging ts-node -r tsconfig-paths/register ./scripts/usersGenerateScript.ts",
    "pm2:prod": "pm2 start pm2system.config.js  --only travoofy_api_production",
    "pm2:dev": "pm2 start pm2system.config.js --only travoofy_api_development"
  },
  "keywords": [],
  "author": "kennyendowed",
  "license": "ISC",
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@metrobuzz/express-routes-loader": "^0.0.1",
    "@types/form-data": "^2.5.2",
    "@types/jwt-decode": "^3.1.0",
    "@types/request-ip": "^0.0.41",
    "axios": "^1.13.2",
    "bcryptjs": "^3.0.3",
    "body-parser": "^2.2.0",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-fileupload": "^1.5.2",
    "express-handlebars": "^7.1.3",
    "express-list-endpoints": "^7.1.1",
    "express-rate-limit": "^8.2.1",
    "fast-csv": "^5.0.5",
    "file-upload": "^0.0.0",
    "form-data": "^4.0.5",
    "handlebars": "^4.7.8",
    "helmet": "^8.1.0",
    "joi": "^18.0.2",
    "joi-to-swagger": "^6.2.0",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "moment": "^2.30.1",
    "mongoose": "^8.20.0",
    "morgan": "^1.10.1",
    "mysql2": "^3.15.3",
    "netwrap": "^3.0.7",
    "niggamomentrouter": "^1.1.13",
    "node-cron": "^4.2.1",
    "nodemailer": "^7.0.10",
    "normal-case-generator": "^1.0.1",
    "openai": "^6.9.1",
    "oracledb": "^6.10.0",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "read-excel-file": "^6.0.1",
    "request-ip": "^3.3.0",
    "rotating-file-stream": "^3.2.7",
    "sequelize": "^6.37.7",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "swagger-jsdoc": "6.2.8",
    "swagger-ui-express": "5.0.1",
    "tedious": "^18.6.1",
    "winston": "^3.18.3",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.28.3",
    "@babel/core": "^7.28.5",
    "@babel/eslint-parser": "^7.28.5",
    "@babel/node": "^7.28.0",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@types/bcryptjs": "^3.0.0",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/crypto-js": "^4.2.2",
    "@types/date-fns": "^2.6.3",
    "@types/dotenv": "^8.2.3",
    "@types/express": "^5.0.5",
    "@types/express-fileupload": "^1.5.1",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/mongoose": "^5.11.97",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.10.1",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^7.0.4",
    "@types/passport": "^1.0.17",
    "@types/passport-google-oauth20": "^2.0.17",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/uuid": "^11.0.0",
    "@types/xml2js": "^0.4.14",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "cross-env": "^10.1.0",
    "esbuild": "^0.24.2",
    "eslint": "^9.39.1",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-prettier": "^5.5.4",
    "lint-staged": "^15.5.2",
    "nodemon": "^3.1.11",
    "prettier": "3.6.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ],
    "*.{json,md,yaml,yml,js,jsx}": [
      "prettier --write"
    ]
  },
  "_moduleAliases": {
    "@": "src",
    "@config": "src/config",
    "@types": "src/types",
    "@constants": "src/constants",
    "@utils": "src/utils",
    "@controllers": "src/controllers",
    "@routers": "src/routers"
  }
}
